<%= form_with model: [@test, @question], local: true do |form| %>
  <%= render 'shared/errors', resource: @question %>
    <p>
      <%= form.label :body %>
      <%= form.text_field :body %>
    </p>
    <p>
      <%= form.label :category %>
      <%= form.text_field :category %>
    </p>
  
    <%= form.submit %>
<% end %>
<br>
